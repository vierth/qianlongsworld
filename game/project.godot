[application]

config/name="Game"
run/main_scene="res://levels/KdLi/building_1.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SceneSwitcher="*res://autoloadscripts/scene_switcher.gd"
SqlController="*res://autoloadscripts/sql_controller.gd"
Settings="*res://autoloadscripts/Settings.gd"
GameState="*res://autoloadscripts/GameState.gd"
GlobalSingleton="*res://autoloadscripts/GlobalSingleton.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"dufufan": "res://characters/npcs/poetryfans/dufufan.dch",
"gongqianyi": "res://characters/npcs/poetryfans/gongqianyi.dch",
"my_new_char_1": "res://my_new_char_1.dch",
"my_new_char_2": "res://my_new_char_2.dch",
